<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:backgroundTint="#104F9B"
    android:background="#5856d6"
    tools:context=".Home">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/ProfilePicFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/PersonalDetailFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/SummaryFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/EducationFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/ExperienceFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/CertificationFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/ReferencesFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/CVpreviewFrag"
/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_button"
        class="com.example.assignement2.ButtonFrag"
        android:id="@+id/DiscarddataFrag"
/>


</androidx.appcompat.widget.LinearLayoutCompat>